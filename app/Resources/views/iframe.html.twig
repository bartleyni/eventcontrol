{% extends 'BathSU.html.twig' %}
{% block headerbottom %}
Event Control
{% endblock %}
{% block layout %}
    {% embed '3-9.html.twig' %}
        {% block content %}
            {% if iframe %}
                {{ iframe|raw }}
                {% if iframe_username %}
                    <script type="text/javascript">
                        $(document).ready(function() {
                                $('#iframe_radar').contents().find('#email_address').val("{{ iframe_username }}");
                                $('#iframe_radar').contents().find('#password').val("{{ iframe_password }}");
                                //$('#email_address').val("{{ iframe_username }}");
                                //$('#password').val("{{ iframe_password }}");
                        }
                    </script>
                {% endif %}
            {% endif %}
            {% if data %}
                {{ data }}
            {% endif %}
        {% endblock %}
        {% block sidebar %}
            {% include 'sidebar.twig' %}
        {% endblock %}
    {% endembed %}
{% endblock %}
