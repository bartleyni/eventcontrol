{% extends 'BathSU.html.twig' %}
{% block headerbottom %}
Event Control
{% endblock %}
{% block layout %}
    {% embed '3-9.html.twig' %}
        {% block content %}
            {% if iframe %}
                {{ iframe|raw }}
                {% if iframe_username %}
                    <script type="text/javascript">
                        $(document).ready(function() {
                                $('#email_address').html({{ iframe_username }});
                                $('#password').html({{ iframe_password }});
                        }
                    </script>
                {% endif %}
            {% endif %}
            {% if data %}
                {{ data }}
            {% endif %}
        {% endblock %}
        {% block sidebar %}
            {% include 'sidebar.twig' %}
        {% endblock %}
    {% endembed %}
{% endblock %}
