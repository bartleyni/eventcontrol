    <div class="panel panel-warning">
        {# <div class="panel-heading">
            UPS Status
        </div>
        #}
        <div class="panel-heading">
            <a data-toggle="collapse" href="#UPScollapse" style="text-decoration:none;">
                <div class="row">
                    UPS Status
                </div>
            </a>
        </div>
        <div id="UPScollapse" class="panel-collapse collapse">
            <ul class="list-group ">
                {% for ups in UPSs %}
                    <li class="list-group-item" id="ups-{{ ups.id }}" data-toggle="tooltip" data-placement="right" title="{{ ups.name }} - {{ ups.location }}">UPS{{ ups.id }}: Timeout</li>
                {% endfor %}
            </ul>
        </div>
    </div>
